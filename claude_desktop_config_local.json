{
  "mcpServers": {
    "baker-virtual-assistant-local": {
      "command": "uv",
      "args": [
        "--directory",
        "/absolute/path/to/mcp-servers/baker-virtual-assistant",
        "run",
        "baker-virtual-assistant"
      ]
    },
    "app-testing-assistant-local": {
      "command": "uv",
      "args": [
        "--directory",
        "/absolute/path/to/mcp-servers/app-testing-assistant",
        "run",
        "app-testing-assistant"
      ]
    },
    "notes-organizer-local": {
      "command": "uv",
      "args": [
        "--directory",
        "/absolute/path/to/mcp-servers/notes-organizer",
        "run",
        "notes-organizer"
      ]
    },
    "hello-world-local": {
      "command": "uv",
      "args": [
        "--directory",
        "/absolute/path/to/mcp-servers/hello-world",
        "run",
        "hello-world"
      ]
    },
    "openai-rag-vector-store-local": {
      "command": "uv",
      "args": [
        "--directory",
        "/absolute/path/to/mcp-servers/openai-rag-vector-store",
        "run",
        "openai-rag-vector-store"
      ],
      "env": {
        "OPENAI_API_KEY": "your-openai-api-key-here"
      }
    }
  }
}
